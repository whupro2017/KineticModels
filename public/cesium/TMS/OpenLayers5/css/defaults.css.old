html, body, #map {
    font-family: "微软雅黑", sans-serif;
    margin: 0;
    height: 100%;
    width: 100%;
    padding: 0;

}

#map {
  background: url(../resources/map-background.jpg) repeat;
}

.hide{
    display: none;
}

@font-face{
    font-family:'heydings';
    src:url('assets/font/heydings.eot');
    src:url('assets/font/heydings.eot') format('embedded-opentype'), url('assets/font/heydings.woff') format('woff'), url('assets/font/heydings.ttf') format('truetype'), url('assets/font/heydings.svg') format('svg');
    font-weight:normal;
    font-style:normal;
}

#header{
    position: absolute;
    top: 0;
    height: 70px;
    background: rgba(100, 100, 100, 0.85);
    width: 100%;
}

#logoandtitle {
    background-image: url(../resources/logo.png);
    background-size: 60px 60px;
    background-position: center left;
    background-repeat: no-repeat;
    padding-left: 70px;
    padding-right: 20px;
    font-family: "微软雅黑";
    font-size: 20px;
    font-weight: bold;
    color: #eeeeee;
    margin: 0;
    margin-left: 10px;
    height: 100%;
    line-height: 50px;
}

#version{
    font-size: 12px;
    margin-top:-28px;
    margin-left:84px;
    color: #eeeeee;
}

#main_menu{
	float:right;
	margin:22px 0 0 0
}
.navi{
	padding:0;
	margin:0;
	list-style:none;
	display: block;
}
.navi-co.co {
    height: auto;
    overflow: visible;
	position:relative;
}
.navi-pi>li{
	float:left;
	margin:0;
	padding:0;
	display:inline-block;
	line-height: 20px;
	text-align: -webkit-match-parent;
}
.navi-pi>li>a{
	font-weight:400;
	color:rgb(238, 238, 238);
	letter-spacing:2px;
	font-size:15px;
	display:block;
	padding:15px 2px 2px 10px;
	text-align:center;
	margin-top: 2px;
    margin-bottom: 2px;
	margin-right: 2px;
	border-radius: 5px;
	text-decoration: none;
	transition: all .3s ease;
    outline: 0;
	cursor: auto;
}
.navi-pi>li>a:hover{
	color:rgb(255, 255, 0);
}
.baseContainer{
    position: absolute;
    top: 80px;
    visibility: hidden;
    padding: 10px;
    color: #cccccc;
}

.baseContent{
    padding-left: 10px;
	padding-top: 10px;
    position: absolute;
    top: 35px;
    opacity: 0.8;
    width: 100%;
    background: rgba(100, 100, 100, 200);
    font-size: 14px;
}

.toolWindowTitle{
    position: absolute;
    top: 0;
    height: 35px;
    line-height: 35px;
    width: 100%;
    background: rgba(100, 100, 100, 255);
    padding-left: 10px;
}

#plotDrawContainer{
    left: 50px;
	width: 700px;
}

#plotDrawContent{
    height: 125px;
}

#animationContainer{
    left: 50px;
	width: 700px;
}

#animationContent{
    height: 125px;
}

#download{
    float: right;
    font-size: 16px;
    cursor: pointer;
    line-height: 70px;
    margin-right: 20px;
}

#aboutContainer{
    right: 70px;
    width: 450px;
}

#aboutContent{
    height: 210px;
}

.aboutText{
    padding-left: 5px;
    padding-right: 10px;
    text-indent: 26px;
}
.aboutText>a{
    color:rgb(255, 255, 255);
}
.aboutText>a:hover{
	color:rgb(255, 255, 0);
}

#closeBtn{
    float: right;
    font-size: 16px;
    cursor: pointer;
    padding-right: 10px;
}

button{
    font-family: "微软雅黑", sans-serif;
    opacity: 1;
    padding: 4px 6px;
}

#delete-wrapper{
    position: absolute;
    bottom: 0;
    width: 100%;
    text-align: center;
    padding-bottom: 8px;
}

#delete-wrapper #btn-delete{
    padding: 8px 16px;
    display: inline-block;
    color: #fff;
    cursor: pointer;
    background: rgba(255, 8, 53, 0.85);
}
